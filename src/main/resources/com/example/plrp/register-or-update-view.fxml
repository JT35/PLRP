<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <Text fx:id="header" strokeType="OUTSIDE" strokeWidth="0.0" text="New Item" BorderPane.alignment="TOP_LEFT">
         <font>
            <Font name="Arial" size="36.0" />
         </font>
         <BorderPane.margin>
            <Insets bottom="80.0" left="8.0" right="8.0" top="8.0" />
         </BorderPane.margin>
      </Text>
   </top>
   <center>
      <VBox prefHeight="184.0" prefWidth="593.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="4.0" left="4.0" right="4.0" top="3.0" />
         </BorderPane.margin>
         <children>
            <TextField fx:id="barcode" editable="false" focusTraversable="false" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="0.0" prefWidth="165.0">
               <VBox.margin>
                  <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
               </VBox.margin>
               <font>
                  <Font name="Arial" size="12.0" />
               </font>
            </TextField>
            <HBox>
               <children>
                  <Label text="Model:">
                     <HBox.margin>
                        <Insets bottom="4.0" left="4.0" right="75.0" top="4.0" />
                     </HBox.margin>
                     <font>
                        <Font name="Arial" size="12.0" />
                     </font></Label>
                  <TextField fx:id="model">
                     <font>
                        <Font name="Arial" size="12.0" />
                     </font></TextField>
               </children>
               <VBox.margin>
                  <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
               </VBox.margin>
            </HBox>
            <HBox layoutX="14.0" layoutY="46.0">
               <children>
                  <Label text="Power Output (PO):">
                     <HBox.margin>
                        <Insets bottom="4.0" left="8.0" right="14.0" top="4.0" />
                     </HBox.margin>
                     <font>
                        <Font name="Arial" size="12.0" />
                     </font>
                  </Label>
                  <TextField fx:id="powerOutput">
                     <font>
                        <Font name="Arial" size="12.0" />
                     </font>
                  </TextField>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="J">
                     <HBox.margin>
                        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
                     </HBox.margin>
                     <font>
                        <Font name="Arial" size="12.0" />
                     </font>
                  </Text>
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
   <bottom>
      <HBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="submitBtn" defaultButton="true" mnemonicParsing="false" onAction="#onSubmitClick" text="Register">
               <font>
                  <Font name="Arial" size="12.0" />
               </font>
               <HBox.margin>
                  <Insets right="4.0" />
               </HBox.margin>
            </Button>
            <Button cancelButton="true" mnemonicParsing="false" onAction="#onCancelClick" text="Cancel">
               <font>
                  <Font name="Arial" size="12.0" />
               </font>
            </Button>
         </children>
         <BorderPane.margin>
            <Insets bottom="148.0" right="4.0" />
         </BorderPane.margin>
      </HBox>
   </bottom>
</BorderPane>
